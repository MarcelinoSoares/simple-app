{
  "all": true,
  "check-coverage": true,
  "reporter": [
    "text",
    "text-summary",
    "lcov",
    "html",
    "json"
  ],
  "exclude": [
    "coverage/**",
    "test/**",
    "**/*.test.js",
    "**/*.spec.js",
    "**/node_modules/**",
    "src/server.js"
  ],
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80
} 